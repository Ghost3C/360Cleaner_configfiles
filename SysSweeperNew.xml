                   if nSplitArray[1] ~= nil and nSplitArray[1] ~= "" and FileExists(nSplitArray[1]) == false and  nSplitArray[2] ~= nil and nSplitArray[2] ~= "" and FileExists(nSplitArray[2]) == false 
