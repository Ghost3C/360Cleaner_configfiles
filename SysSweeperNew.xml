                   elseif nSplitArray[1] == "" and  nSplitArray[2] == "" and  nSplitArray[3] == "" and  nSplitArray[4] == "" and  nSplitArray[5] == "" and  nSplitArray[6] == "" then
                            path = nSplitArray[7].."|"..nSplitArray[8].."|"..nSplitArray[9].."|"..nSplitArray[10]
                   end
