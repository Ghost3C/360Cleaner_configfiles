          <work>
            <folder2 path="%variant9%">
              <variant id="1" type="reg" path="HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Windows\CurrentVersion\Uninstall\YoukuClient" key1="DisplayIcon"/>
              <variant id="2" type="reg" path="HKEY_CURRENT_USER\Software\YouKu\YoukuClient" key1="Path"/>
              <variant id="3" type="lua" path="%personal%\Youku Files"/>
              <variant id="4" type="lua" path="%appdata%\youku"/>
              <variant id="5" type="lua" path="%ProgramFILES%\youku\YoukuClient"/>
              <variant id="6" type="lua" path="%Hard%Youku Files\download\temp"/>
              <variant id="7" type="lua" path="%Hard%Youku Files\youkudisk"/>
              <variant id="8" type="lua" path="%variant1%;%variant2%;%variant3%;%variant4%;%variant5%;%variant6%;%variant7%">
				local szFullString = path
				local  szSeparator = ";"
				local nFindStartIndex = 1
				local nSplitIndex = 1
				local nSplitArray = {}
				while true do
					local nFindLastIndex = string.find(szFullString, szSeparator, nFindStartIndex)
					if not nFindLastIndex then
						nSplitArray[nSplitIndex] = string.sub(szFullString, nFindStartIndex, string.len(szFullString))
							break
						end
						nSplitArray[nSplitIndex] = string.sub(szFullString, nFindStartIndex, nFindLastIndex - 1)
						nFindStartIndex = nFindLastIndex + string.len(szSeparator)
					nSplitIndex = nSplitIndex + 1
				end

				path=""

				if #nSplitArray == 7 then
                   if nSplitArray[1] ~= nil and nSplitArray[1] ~= "" and FileExists(nSplitArray[1]) == false and  nSplitArray[2] ~= nil and nSplitArray[2] ~= "" and FileExists(nSplitArray[2]) == false then
                            path = nSplitArray[3].."|"..nSplitArray[4].."|"..nSplitArray[5].."|"..nSplitArray[6].."|"..nSplitArray[7]
                   elseif nSplitArray[1] == "" and  nSplitArray[2] == "" then
                            path = nSplitArray[3].."|"..nSplitArray[4].."|"..nSplitArray[5].."|"..nSplitArray[6].."|"..nSplitArray[7]
                   end
				end
			</variant>
              <variant id="9" type="lua" path="%variant8%" key1="|">
                 path=path
            </variant>
            </folder2>
            <filter>*.*</filter>
